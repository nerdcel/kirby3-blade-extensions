{
  "name": "nerdcel/kirby3-blade-extensions",
  "description": "Kirby3 plugin to add directives and ifs to blade.",
  "license": "MIT",
  "type": "kirby-plugin",
  "version": "1.0.2",
  "keywords": [
    "kirby3",
    "kirby-cms",
    "kirby-plugin",
    "getkirby",
    "kirby"
  ],
  "homepage": "https://github.com/nerdcel/kirby3-blade-extensions",
  "authors": [
    {
      "name": "Marcel Hieke",
      "email": "office@marcelhieke.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "illuminate/view": "^9.0",
    "illuminate/config": "^9.0",
    "matthiasmullie/minify": "^1.3.70",
    "getkirby/composer-installer": "^1.2.1"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest"
  },
  "autoload": {
    "psr-4": {
      "Nerdcel\\BladeExtensions\\": "src/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "allow-plugins": {
      "getkirby/composer-installer": true
    }
  },
  "extra": {
    "installer-name": "kirby3-blade-extensions"
  }
}
